numero1 = int(input('Digite um numero(1): '))
numero2 = int(input('Digite um numero(2): '))

if numero1>numero2:
    print(numero1)
elif numero2>numero1: 
    print(numero2)
2)
numero1 = int(input('Digite um numero(1): '))
numero2 = int(input('Digite um numero(2): '))
numero3 = int(input('Digite um numero(3): '))

maior = numero1
if numero2> maior:
    maior = numero2
if numero3>maior:
    maior = numero3

print(maior)
