  programa 
{
  funcao inicio() 
  {
   real largura, comprimento, area, perimetro 
   escreva("Escreva a largura do terreno:")
   leia(largura)
   escreva("Escreva o comprimento do terreno:")
   leia(comprimento)
   
   area = largura * comprimento
   perimetro = (2*largura) + (2 * comprimento)

   se(largura < 10 )
   {
    escreva("O terreno não e comercial")
   }
   senao
   {
    escreva("O terreno é comercial")
       se(area < 100)
   {
    escreva("\nTERRENO POPULAR")
   }
   senao se(area>100 e area<500)
   {
    escreva("\nTERRENO MASTER")
   }
   senao
   {
    escreva("\nTERRENO VIP")
   }
   escreva("\nO perimetro do terreno é :",perimetro)
   
   }
  }
}
d)
programa 
{
  funcao inicio() 
  {
    cadeia fun
    inteiro salario, anos, novoSalario
    caracter cargo 
    escreva("Digite o nome do funcionário: ")
    leia(fun)
    escreva("Digite o salário que ele recebe: ")
    leia(salario)
    escreva("Digite o tempo que ele trablha na empresa: ")
    leia(anos)
    escreva("Digite que tipo de cargo ele está: ")
    leia(cargo)
    
    
    se(anos <=3)
    {
     novoSalario=salario + (salario*0.03)
     escreva("O novo salário será: ",novoSalario)
    
     
    }
    senao se(anos>=3 e anos <10)
    {
      
     novoSalario=salario + (salario*0.13)
     escreva("O novo salário será: ",novoSalario)
    }
    senao
    {
     
       novoSalario=salario + (salario*0.20)
    }
    se(cargo=='o')
    {
      novoSalario = novoSalario + (novoSalario*0.05)
    }
    
    escreva("\nComo o funcionario é um operador há um aumento de 5%, logo o novo salario sera:", novoSalario)

  }
}
e)
programa 
{
  funcao inicio() 
  {
    inteiro a,b,c,d, r1,r2,r3,r4,r5,r6, maiorProduto
    escreva("Digite 4 números inteiros:\n")
    leia(a,b,c,d)

    r1 = a*b
    r2 = a*c 
    r3 = a*d 
    r4 = b*c 
    r5 = b*d
    r6 = c*d
     
    maiorProduto=r1
    se(r2 > maiorProduto)
    {
     maiorProduto=r2
    }
     se(r3 > maiorProduto)
    {
      maiorProduto=r3
    }
     se(r4> maiorProduto)
    {
     maiorProduto=r4
    }
     se(r5>maiorProduto)
    {
      maiorProduto=r5
    }
     se(r6>maiorProduto)
    {
      maiorProduto=r6
    }
   
    escreva("O maior produto é: ",maiorProduto)
  }
}
f)
programa 
{
  funcao inicio() 
  {
    inteiro a,b,c,d, d1,d2
    escreva("Digite 4 numeros inteiros:\n")
    leia(a,b,c,d)

    d1= a/b
    d2= c/d
    se(d1>3)
    {
      escreva("O resultado é: ",d1)
    }
    senao se(d1<=3)
    {
      se(d2>3)
      {
        escreva("O resultado é: ",d2)
      }
    }
    se(d1>3 e d2 >3)
    {
     escreva("\nProdutos maiores que 3")
    }
    senao
    {
      escreva("\nPelo menos um dos produtos é menor que 3")
    }


  }
}

