1)
n = int(input("Digite um número que deseja saber a tabuada até o 10:"))
for i in range(1,11):
    resultado = n*i
    print(resultado)
2)
n = int(input("Digite um número inteiro:"))
soma=0
for i in range (1,n+1):
    soma +=i
    print(soma)
3)
a = int(input("Digite o valor para A: "))
b = int(input("Digite o valor para B(observação B tem que ser maior q A):" ))

if a>b:
    print("Falha voce A maior que o numero B")
    
else:
    for i in range(a,b+1):
        print(i)
4)
n = int(input("Digite um número inteiro: "))
fat=1
for i in range(1,n+1):
    fat*=i
    print(fat)
5)
n = int(input("Digite o número de linhas que deseja na pirâmide: "))

for i in range(1,n+1):
    espacos=''*(n-1)
    asteriscos='*'*(2*i-1)
    print(espacos+asteriscos)
