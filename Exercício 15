pesoPeixe=int(input('Digite o peso do peixe em KG:'))

if pesoPeixe>50:
    excesso=pesoPeixe -50
    print('Excesso de: ',excesso)
    multa=excesso * 4
    print('A multa será de: ',multa)
else:
    print('Não a multa para pagar')
9)
ganhoPorhora= int(input('Digite o seu ganho por hora: '))
horaPorMes=int(input('Digite quantas horas trabalha por mes: '))

ganhoTotal = ganhoPorhora * horaPorMes
print('Salario bruto: ',ganhoTotal)

pagoParaINSS = ganhoTotal*0.08
print('Valor para para o INSS:',pagoParaINSS)
pagoParaIR = ganhoTotal*0.11
print('Valor pago para IR:',pagoParaIR)

pagoParaSindicato = ganhoTotal*0.5
print('Valor pago para sindicato:',pagoParaSindicato)

salarioLiquido = ganhoTotal -(pagoParaIR + pagoParaSindicato +pagoParaINSS)
print("O salario liquido é: ",salarioLiquido)
